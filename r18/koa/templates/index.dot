<!DOCTYPE html>
<html lang="{{= it.zh ? 'zh' : 'en'}}">
<head>
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"/>
	<title>
		JvrLibrary&nbsp;
		{{? it.type === 'jvr' }}
			{{=it.pageTitle}}
		{{?? it.pageTitle && it.pageTitle !== 'ranking'}}
			{{= it.pageTitle + ' archive - '}}
		{{?? it.pageTitle && it.pageTitle === 'ranking'}}
			{{=it.zh ? '排行' : 'Ranking'}} -
		{{?}}	
	</title>
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<link rel="icon" type="image/png" href="/static/favicon.png">
	<meta name="description" content="Japan VR Adult Videos Info, Rapidgator and Torrent">
	<link rel="canonical" href="http://www.jvrlibrary.com{{=it.currentUrl}}" />
	<link href="/static/index9.css" rel="stylesheet" type="text/css"/>
	<link href="/static/webfonts/all.min.css" rel="stylesheet" type="text/css"/>
	<script src="/static/jquery.min.js"></script>
	<!-- Global site tag (gtag.js) - Google Analytics -->
	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-145060231-2"></script>
	<script>
	  window.dataLayer = window.dataLayer || [];
	  function gtag(){dataLayer.push(arguments);}
	  gtag('js', new Date());

	  gtag('config', 'UA-145060231-2');
	</script>
	<!--[if lt IE 10]><style>
		.search input {
			height: 32px;
			line-height: normal;
		}
	</style><![endif]-->
</head>
<body>
<div id='root'>
	{{#def.sideMenu}}
	<div class="main {{=it.type === 'jvr' ? 'jvr' : 'normal'}} {{=it.type === 'jvrList' ? 'jvrList' : ''}}">
	
		{{? it.type === 'jvr'}}
			{{#def.singleView}}
		{{?? it.type === 'actress'}}
			{{#def.actressView}}
			{{#def.pagination}}
		{{?? it.type === 'category'}}
			{{#def.categoryView}}
		{{?? it.type === 'studio'}}
			{{#def.studioView}}
			{{#def.pagination}}
		{{?? it.type === 'ranking'}}
			{{#def.ranking}}
		{{??}}
			{{#def.listView}}
			{{#def.mySelectionAndCurrentClicks}}
		{{?}}
	</div>
	<div class="footer">
		{{=it.zh ? '酒后表演艺术家酿造' : 'Brewed by ProfessionalDrunker.' }}
	</div>
	<div id="instant-message"></div>
</div>

<script>
	var code = "{{=it.r18 && it.r18.code || ""}}";
	$(document).ready(function() {
		$("#idSearch").bind('keypress', function(e) {
			if (e.keyCode === 13) {
				let inputValue = $(this).val();
				if (inputValue.match(/[a-zA-Z]+/)) {
					location.href = "/search?search=" + encodeURIComponent(inputValue);  
				} else {
					alert('Sorry, this will not work')
				}	 
			}
		});			 
	})
</script>
<script src="/static/cursor1.js"></script>
<script src="/static/bundle421a14f9505c0c02ee20.js"></script>
</body>
</html>
