<html>
<head>
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"/>
	<title>
		{{? it.type === 'jvr' }}
			{{=it.pageTitle}}
		{{?? it.pageTitle && it.pageTitle !== 'ranking'}}
			{{= it.pageTitle + ' archive - '}}
		{{?? it.pageTitle && it.pageTitle === 'ranking'}}
			{{=it.zh ? '排行' : 'Ranking'}} -
		{{?}}
		&nbsp;JvrLibrary
	</title>
	<link rel="icon" type="image/png" href="/static/favicon.png">
	<meta name="description" content="Japan VR Adult Videos and Torrents">
	<link rel="canonical" href="http://www.jvrlibrary.com{{=it.currentUrl}}" />
	<link href="/static/index.css" rel="stylesheet" type="text/css"/>
	<link href="/static/webfonts/all.min.css" rel="stylesheet" type="text/css"/>
	<script
	  src="https://code.jquery.com/jquery-3.4.1.min.js"
	  integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
	  crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/gh/StephanWagner/jBox@v1.0.5/dist/jBox.all.min.js"></script>
	<link href="https://cdn.jsdelivr.net/gh/StephanWagner/jBox@v1.0.5/dist/jBox.all.min.css" rel="stylesheet">
	<script type="text/javascript" src="/static/swiper.min.js" ></script>
	<link href="/static/swiper.min.css" rel="stylesheet" type="text/css"/>
	<script type="text/javascript" src="/static/react.development.js" ></script>
	<script type="text/javascript" src="/static/react-dom.development.js" ></script>
	<script type="text/javascript" src="/static/babel.min.js" ></script>
	<script type="text/javascript" src="/static/axios.min.js" ></script>
	{{? !it.isBot }}
		<script type="text/babel" src="/static/main.jsx"></script>
		<script type="text/babel" src="/static/swiperInit.js"></script>
	{{?}}
	<script type="text/javascript" src="/static/dayjs.min.js"></script>	
	<script src="/static/socket.io.dev.js"></script>

	<script>
		var code = "{{=it.r18 && it.r18.code || ""}}";
		$(document).ready(function() {
			$("#idSearch").bind('keypress', function(e) {
				if (e.keyCode === 13) {
					let inputValue = $(this).val();
					if (inputValue.match(/[a-zA-Z]+/)) {
						location.href = "/search?search=" + encodeURIComponent(inputValue);  
					} else {
						alert('Sorry, this will not work')
					}	 
				}
			});
			new jBox('Image');	

			if ($("#video-button")) {
				window.jBoxVideo = new jBox('Modal', {
					width: '100%',
					height: 'auto',
					attach: '#video-button',
					content: '<video controls><source src="{{= it.r18 && it.r18.video}}"/></div>',
					onClose: () => {
						$("video").get(0) && $('video').get(0).pause();
					}
				});
			}
			 
		})
	</script>
	<!-- Global site tag (gtag.js) - Google Analytics -->
	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-145060231-2"></script>
	<script>
	  window.dataLayer = window.dataLayer || [];
	  function gtag(){dataLayer.push(arguments);}
	  gtag('js', new Date());

	  gtag('config', 'UA-145060231-2');
	</script>
</head>
<body>
<div id='root'>
	{{#def.sideMenu}}
	<div class="main {{=it.type === 'jvr' ? 'jvr' : 'normal'}} {{=it.type === 'jvrList' ? 'jvrList' : ''}}">
	
		{{? it.type === 'jvr'}}
			{{#def.singleView}}
		{{?? it.type === 'actress'}}
			{{#def.actressView}}
			{{#def.pagination}}
		{{?? it.type === 'category'}}
			{{#def.categoryView}}
		{{?? it.type === 'studio'}}
			{{#def.studioView}}
			{{#def.pagination}}
		{{?? it.type === 'ranking'}}
			{{#def.ranking}}
		{{??}}
			{{#def.listView}}
			{{#def.mySelection}}
		{{?}}
	</div>
	<div class="footer">
		{{=it.zh ? '酒后表演艺术家酿造' : 'Brewed by ProfessionalDrunker.' }}
	</div>
	<div id="instant-message"></div>
</div>
</body>
</html>
