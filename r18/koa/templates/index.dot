<html>
<head>
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"/>
	<title>JvrLibrary</title>
	<meta name="description" content="Japan VR Adult Videos">
	<meta name="google-site-verification" content="CIXkjavTmo_tagn3LgFMI1H81AwF88_iTgiFCXE2-UU" />
	<link rel="canonical" href="http://www.jvrlibrary.com/">
	<link href="/static/index.css" rel="stylesheet" type="text/css"/>
	<script
	  src="https://code.jquery.com/jquery-3.4.1.min.js"
	  integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
	  crossorigin="anonymous"></script>
	<script>
		$(document).ready(function() {
			$("#idSearch").bind('keypress', function(e) {
				if (e.keyCode === 13) {
					let inputValue = $(this).val();
					if (inputValue.match(/[a-zA-Z]+/)) {
						location.href = "/search?search=" + encodeURIComponent(inputValue);  
					} else {
						alert('Sorry, this will not work')
					}	 
				}
			});	
		})
	</script>
</head>
<body>
<div id='root'>
	{{#def.sideMenu}}
	<div class="main">
	
		{{? it.type === 'jvr'}}
			{{#def.singleView}}
		{{?? it.type === 'actress'}}
			{{#def.actressView}}
			{{#def.pagination}}
		{{?? it.type === 'category'}}
			{{#def.categoryView}}
		{{??}}
			{{#def.listView}}
			{{#def.pagination}}
		{{?}}
	</div>
	<div class="footer">
		Brewed by ProfessionalDrunker.
	</div>
</div>
</body>
</html>
