<div class="table">
  <h5>Current Task: {{=it.currentBackgroundTask}}</h5>
  <h5>Disable Post: {{=disablePost}}</h5>
  <p>{{=it.lastLogs}}</p>
  {{~it.tables :table}}
    <table style="border:1px solid; margin-bottom: 20px; font-size: 10px;">
      {{~table :row}}
        <tr>
          {{~row :item}}
            <td style="padding: 0 5px;">{{=item}}</td>
          {{~}}
        </tr>
      {{~}}
    </table>
  {{~}}
</div>