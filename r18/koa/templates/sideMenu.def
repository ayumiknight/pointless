<script>
	window.getCookie = function(name) {
		var match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));
		if (match) return match[2];
	};
	window.setCookie = function(key, value, times) {
		var date = new Date();
		date.setTime(date.getTime() + times * 1000); 
		document.cookie = key + "=" + encodeURIComponent(value) + ";path=/;expires=" + date.toGMTString();
	};
	window.setNonVR = function(on) {
		let current = !!getCookie('nonVR');
		console.log('invoked');
		if (true || current != on) {
			setCookie('nonVR', on ? 'true' : '', 60 * 60 * 24 * 30);
			setTimeout(function() {
				location.href = location.origin;
			}, 0);
		}
	}
</script>