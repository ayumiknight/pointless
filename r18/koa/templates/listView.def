<div class="jvr-list">
	<div class="page-title">{{=it.pageTitle}}</div>
	<div class="list-view f r fc">

		{{~it.r18s :value:index}}
			<a class="list-item f" href="{{=it.zh ? '/zh' : ''}}/jvr?id={{=encodeURIComponent(value.code)}}">
				<img src="{{=value.cover}}" />
				<div class="bottom">
					<div class="code">
						{{=value.code}}
					</div>

					<div class="title">
						{{=it.zh ? value.zhTitle : value.title}}
					</div>	
				</div>
			</a>
		{{~}}
		{{? it.r18s.length === 0}}
			{{#def.empty}}
		{{?}}
	</div>
</div>

