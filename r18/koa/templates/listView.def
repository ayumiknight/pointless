
<div class="list-view">
	{{~it.r18s :value:index}}
		<a class="list-item" href="/jvr?id={{=encodeURIComponent(value.code)}}">
			<img src="{{=value.cover}}" />
			<div class="bottom">
				<div class="code">
					{{=value.code}}
				</div>

				<div class="title">
					{{=value.title}}
				</div>	
			</div>
		</a>
	{{~}}
</div>

