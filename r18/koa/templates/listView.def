<div class="jvr-list">
	{{? it.pageTitle}}
		<div class="page-title">{{=it.pageTitle}}</div>
	{{?}}
	<div class="list-view f r fc">
		{{~it.r18s :value:index}}
			<a class="list-item f" href="{{=it.zh ? '/zh' : ''}}/jvr?id={{=encodeURIComponent(value.code)}}">
				<img alt="{{=value.code}} small cover image" class="cover" src="{{#def.proxy:value.Galleries[0].url}}"/>
				{{?value.newTag}}
				<span class="new-tag">NEW TODAY</span>
				{{?}}
				<div class="bottom">
					<div class="code">
						{{=value.code}}
					</div>

					<div class="title">
						{{=(it.zh ? value.zhTitle : value.title) || it.title}}
					</div>	
				</div>
			</a>
		{{~}}
		{{? it.r18s.length === 0}}
			{{#def.empty}}
		{{?}}
	</div>
	{{#def.pagination}}
</div>

