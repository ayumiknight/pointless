<div class="actress-view f c fc">
	<div class="page-title">{{=it.zh ? '女优' : 'Casts'}}</div>
	<div class="actress-pagination f r fc">
		{{~ it.letterPagination :value:index}}
			<div class="pagination f fc {{=value.className}}">
				<a href="{{=value.url}}">{{=value.letter}}</a>
			</div>
		{{~}}
	</div>
	<div class="actress-lists f r fc">
		{{~it.actresses :value:index}}
			<a class="list-item" href="{{=it.zh ? '/zh' : ''}}/cast?cast={{=encodeURIComponent(it.zh ? value.zh : value.en)}}">
				<img src="{{=value.logo}}"/>
				<div class="bottom">
					<div class="title">
						{{=it.zh ? value.zh : value.en}}
					</div>	
				</div>
			</a>
		{{~}}
	</div>
</div>

